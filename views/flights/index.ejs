<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
     
      <th>Airline</th>
      <th>Airport</th>
      <th>Flight<br>Number</th>
      <th>Departs</th>
     

      <th></th>

    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(f) { %>
      <tr>
        <td><%= f.airline %></td>
        <td><%= f.airport %></td>
        <td><%= f.flightNo %></td>
        <td><%= f.departs %></td>
        <!-- <td><%= f.arrivals %></td> -->
        
        <td><a href="/flights/<%= f.id %>">DETAILS</a></td>
      </tr>
    <% }); %> 

      <%- include('../partials/footer') %>
      